  <body>
    <header>
            
        <div id="logo" class="menuUp">

            <h1>FUNNY TIMES</h1>
                            
            <div id="navToggle"><a href="#">Menu</a></div>
        </div>
        <nav>
            <ul>
                <li><a href="/">Home</a></li>
                <li><a href="/profile">Profile</a></li>
                <li><a href="/jokes">New Jokes</a></li>
                 <li><a href="/gifs">GIFS</a></li>
                <li>
                  {{#if user}}
