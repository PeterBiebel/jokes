                    {{else}}
                     <a href="#">Sign Up<span class="toggle">Expand</span><span class="caret"></span></a>
                     {{/if}}
                    <nav>
                        <ul>
                            {{#if user}}
                              
                             <li> <a href="/logout">LogOut</a></li>
                             {{else}}
                             <li> <a href="/register">Register</a></li>
                             <li> <a href="/login">Log In</a></li>
                            {{/if}}
                            
                            
                           
                        </ul>
                    </nav>
